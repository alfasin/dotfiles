# PATH additions
for another_bin in \
    $HOME/bin \
    $HOME/.rbenv/bin
do
    [[ -e $another_bin ]] && export PATH=$another_bin:$PATH
done

# Add brew coreutils to the PATH
brew --prefix coreutils && export PATH=$(brew --prefix coreutils)/libexec/gnubin:$PATH

export PATH=/usr/local/bin:/usr/local/sbin:$PATH